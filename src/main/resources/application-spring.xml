<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 配置 -->
    <bean id="baseConfig" class="com.shinemo.baas.openapi.client.common.BaseConfig">
        <property name="openApiUri" value="${openApiUri}"/>
        <property name="appId" value="${appId}"/>
        <property name="appSecret" value="${appSecret}"/>
    </bean>

    <bean id="contactClient" class="com.shinemo.baas.openapi.client.client.ContactClient">
        <constructor-arg ref="baseConfig"/>
    </bean>
</beans>